            <if test="groupCode != null">
                #{groupCode, jdbcType=VARCHAR},
            </if>
            <if test="code != null">
                #{code, jdbcType=VARCHAR},
            </if>
            <if test="value != null">
                #{value, jdbcType=VARCHAR},
            </if>
            <if test="createAt != null">
                #{createAt, jdbcType=BIGINT},
