    <select id="countByParams" parameterType="java.util.HashMap" resultType="java.lang.Integer">
        SELECT count(*)
        FROM dictionary t
        WHERE 1=1
        <include refid="pageParams"></include>
    </select>

    <select id="listByParams" parameterType="java.util.HashMap" resultMap="DictionaryMap">
        SELECT  t.*
        FROM dictionary t
        WHERE 1=1
        <include refid="pageParams"></include>
        <include refid="orderById"></include>
