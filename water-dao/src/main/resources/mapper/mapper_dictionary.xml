    <sql id="pageParams">
        <if test="id != null">
            and t.id = #{id,jdbcType=BIGINT}
        </if>
        <if test="groupCode != null">
            and t.group_code = #{groupCode,jdbcType=VARCHAR}
        </if>
         <if test="groupCodeLike != null">
